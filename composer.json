{
    "name": "lwj/laravel-rbac",
    "description": "基于权限",
    "keywords": ["laravel", "queue"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jiang Zhiheng",
            "email": "j5110021@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7|^4.0|^5.0",
        "phpunit/phpunit": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lwj\\Rbac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lwj\\Rbac\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Lwj\\Rbac\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
